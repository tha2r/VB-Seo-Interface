RewriteEngine On
RewriteRule ^index.html(.*)$ index.php?act=home&$1
RewriteRule ^tt([0-9][0-9]*)/(.*).html(.*)$ index.php?act=thread&id=$1&$2&$3
RewriteRule ^ff([0-9][0-9]*)/(.*).html(.*)$ index.php?act=forum&id=$1&$2&$3
RewriteRule ^ff([0-9][0-9]*)-([0-9][0-9]*)/(.*).html(.*)$ index.php?act=forum&id=$1&page=$2&$3&$4
RewriteRule ^rss_feeds.xml(.*)$ index.php?act=rss&$1 
